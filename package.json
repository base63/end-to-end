    "dependencies": {
        "chai": "=4.1.2",
        "chai-string": "=1.4.0",
        "cypress": "=1.4.0",
        "mocha": "=4.0.1"
    },
    "devDependencies": {
        "@types/bluebird": "=3.5.19",
        "@types/chai": "=4.0.8",
        "@types/mocha": "=2.2.44",
