{
    "name": "@base63/end-to-end",
    "version": "0.0.0",
    "description": "End-to-end tests of the whole base63",
    "author": "Base63 team",
    "scripts": {
        "build": "tsc",
        "run-prod": "tsc & cypress run --record",
        "run-dev": "tsc --watch & cypress open"
    },
    "keywords": [
        "common",
        "client",
        "tests"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/base63/end-to-end.git"
    },
    "homepage": "https://github.com/base63/end-to-end#readme",
